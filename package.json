{
  "name": "car-front",
  "version": "0.0.0",
  "scripts": {
    "extract": "ng xi18n --output-path src/locale",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path=dist/browser/en --baseHref /en/",
    "build-locale:pl": "ng build --prod --i18n-locale pl --i18n-format xlf --i18n-file src/locale/messages.pl.xlf --output-path=dist/browser/pl --baseHref /pl/",
    "build-locale": "npm run build-locale:pl && npm run build-locale:en",
    "build:client-and-server-bundles-i18n": "ng run CarFront:build:production-pl && ng run CarFront:build:production-en && ng run CarFront:server:production",
    "build:client-and-server-bundles": "ng build --prod && ng run CarFront:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server",
    "compile:server": "tsc -p tsconfig.server.json",
    "serve:ssr": "node dist/CarFront-server/main.js",
    "dev:ssr": "ng run CarFront:serve-ssr",
    "prerender": "ng run CarFront:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@progress/kendo-angular-common": "^1.0.0",
    "@progress/kendo-angular-popup": "^3.0.6",
    "@progress/kendo-theme-default": "latest",
    "express": "^4.17.1",
    "firebase": ">= 5.5.7 <8",
    "hammerjs": "^2.0.8",
    "ng-recaptcha": "^5.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "^0.1000.0",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@nguniversal/builders": "^9.1.1",
    "@nguniversal/express-engine": "^9.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "firebase-tools": "^7.12.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.8",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
